   "source": [
    "def oklidhesaplama(points):\n",
    "    x1,y1,x2,y2= points\n",
    "    a=x2-x1\n",
    "    b=y2-y1\n",
    "    oklid=(a**2 + b**2)**0.5\n",
    "    return oklid"
   ]
